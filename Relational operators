#include <stdio.h>
int main()
{
	int num1, num2;
	printf("Enter two numbers: ");
	scanf("%d %d", &num1, &num2);
	if(num1<num2){
		printf("%d < %d: True\n",num1,num2);
	} else{
		printf("%d < %d: False\n", num1, num2);
	}
	if(num1<=num2){
	printf("%d <= %d: True\n", num1, num2);
	} else{
		printf("%d <= %d: False\n", num1, num2);
	}
	if(num1>num2){
		printf("%d > %d: True\n", num1, num2);
	} else{
		printf("%d > %d: False\n", num1, num2);
	}
	if(num1>=num2){
		printf("%d >= %d: True\n", num1, num2);
	} else {
		printf("%d >= %d: False\n", num1, num2);
	}
	if(num1==num2){
		printf("%d == %d: True\n", num1, num2);
	} else {
		printf("%d == %d: False\n", num1, num2);
	}
	if(num1!=num2){
		printf("%d != %d: True\n", num1, num2);
	} else {
		printf("%d != %d: False\n", num1, num2);
	}
return 0;
}
